#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/james/Documents/works/2024/jameshsu/game-base-template-0926/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3/node_modules/rollup-plugin-visualizer/dist/bin/node_modules:/Users/james/Documents/works/2024/jameshsu/game-base-template-0926/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3/node_modules/rollup-plugin-visualizer/dist/node_modules:/Users/james/Documents/works/2024/jameshsu/game-base-template-0926/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3/node_modules/rollup-plugin-visualizer/node_modules:/Users/james/Documents/works/2024/jameshsu/game-base-template-0926/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3/node_modules:/Users/james/Documents/works/2024/jameshsu/game-base-template-0926/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/james/Documents/works/2024/jameshsu/game-base-template-0926/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3/node_modules/rollup-plugin-visualizer/dist/bin/node_modules:/Users/james/Documents/works/2024/jameshsu/game-base-template-0926/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3/node_modules/rollup-plugin-visualizer/dist/node_modules:/Users/james/Documents/works/2024/jameshsu/game-base-template-0926/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3/node_modules/rollup-plugin-visualizer/node_modules:/Users/james/Documents/works/2024/jameshsu/game-base-template-0926/node_modules/.pnpm/rollup-plugin-visualizer@6.0.3/node_modules:/Users/james/Documents/works/2024/jameshsu/game-base-template-0926/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/cli.js" "$@"
else
  exec node  "$basedir/../../dist/bin/cli.js" "$@"
fi
